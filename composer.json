{
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.65",
        "glpi-project/tools": "^0.7.4",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.11"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.0"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "GlpiPlugin\\Centreon\\": "src"
        }
    }
}
